{
	"name": "ambilab-2024-sveltekit",
	"version": "0.0.1",
	"private": false,
	"engines": {
		"node": ">=18.18.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
