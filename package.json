		"@sveltejs/kit": "^2.5.9",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@types/eslint": "^8.56.10",
		"eslint": "^9.3.0",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "^4.2.17",
		"svelte-check": "^3.7.1",
		"svelte-eslint-parser": "^0.36.0",
		"typescript": "^5.4.5",
		"vite": "^5.2.11"
	},
	"type": "module"
}
